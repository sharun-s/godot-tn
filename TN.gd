var histcache:={}
func get_history(dd):
	var cur_year=0
	var text:=''
	if dd in histcache:
		return histcache[dd]
	while true:
		if cur_year>=len(years):
			histcache[dd]=text
			return text
		if dd in dhistory[cur_year]:
			if cur_year==0:
				text='Created in ' + years[cur_year] +'\n'
			else:
				text=text+'Divided in '+ years[cur_year] + ' with '  
				var tmp=dhistory[cur_year].keys().duplicate()
				tmp.erase(dd)
				text=text +' '+ str(tmp).replace('[','').replace(']','') +'\n'	
		cur_year=cur_year+1
		
